.spyderproject
.spyproject

# Rope project settings
.ropeproject

# mkdocs documentation
/site

# mypy
.mypy_cache/
.dmypy.json
dmypy.json

# Pyre type checker
.pyre/

# ============================================================================
# Mass Spectrometry Data Files (LARGE FILES!)
# ============================================================================
# Raw data files - TOO LARGE for GitHub
*.mzML
*.mzXML
*.raw
*.RAW
*.wiff
*.d/
*.mzData
*.mzML.gz
*.mzXML.gz

# Processed data files (unless specifically needed)
*.mgf
*.ms2
*.pkl
*.pickle

# ============================================================================
# Data Directories (contain large datasets)
# ============================================================================
data/raw/*
data/processed/*
!data/raw/.gitkeep
!data/processed/.gitkeep

# Keep example/test data (small files only)
!data/raw/example_*.mzML
!data/processed/example_*.csv

# Databases (usually large)
data/databases/*
!data/databases/.gitkeep
!data/databases/README.md

# ============================================================================
# Results and Output Files
# ============================================================================
results/
outputs/
figures/
plots/
reports/*.pdf
reports/*.html
!reports/example_report.html

# ============================================================================
# Logs and Cache
# ============================================================================
logs/
*.log
cache/
.cache/
tmp/
temp/

# ============================================================================
# Model Files (can be large)
# ============================================================================
# Trained models
*.h5
*.hdf5
*.pt
*.pth
*.ckpt
*.pb
models/saved_models/*
!models/.gitkeep

# Model checkpoints
checkpoints/

# ============================================================================
# IDE and Editor Files
# ============================================================================
# VSCode
.vscode/
*.code-workspace

# PyCharm
.idea/
*.iml
*.iws
*.ipr

# Sublime Text
*.sublime-project
*.sublime-workspace

# Vim
*~
*.swp
*.swo

# Emacs
*~
\#*\#
/.emacs.desktop
/.emacs.desktop.lock
*.elc

# ============================================================================
